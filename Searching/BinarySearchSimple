

class BinarySearchSimple
{

	public static void main(String args[])
	{
		int arr[] = new int[]{10,20,30,40};
		int key=20;
		l=0;
		h=arr.length-1;
		
		while(l<=h)
		{
			int m=l+(h-l)/2;
			
			if(arr[m]==key)
			System.out.println(key);
			
			if(key>m)
			l=m+1;
			
			if(key<m)
			h=m-1;
		}
		
	
	}







}